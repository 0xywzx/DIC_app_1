<h3>以下の内容で、送信する。</h3>
<%= form_with(model: @contact, local: true, url: contacts_path) do |form| %>

  <%= form.hidden_field :name %>
  <%= form.hidden_field :email %>
  <%= form.hidden_field :content %>

  <p>名前:<%= @contact.name %></p>
  <p>科目名:<%= @contact.email %></p>
  <p>評価:<%= @contact.content %></p>
  <%= form.submit  "登録する" %>

<% end %>
